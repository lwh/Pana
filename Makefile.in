# @configure_input@
#
# Package-related 
package = @PACKAGE_NAME@
version = @PACKAGE_VERSION@

# Prefix-related
prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
subdirs = src doc po

pana: all

Makefile: Makefile.in config.status
	./config.status $@

config.status: configure
	./config.status --recheck

distclean: 
	rm -rf autom4te.cache config.log config.status configure Makefile autoscan configure.scan config.h
	# for subdirs make distclean

clean:

all: 
